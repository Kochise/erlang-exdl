<tr><td class="line">75</td><td><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; Cube *cube = <span class="kw3">new</span> Cube<span class="br0">&#40;</span>_world,_space,<span class="nu0">0</span>.<span class="nu0">1</span>,<span class="nu0">0</span>.<span class="nu0">1</span>,<span class="nu0">0</span>.<span class="nu0">1</span>,<span class="nu0">1</span><span class="br0">&#41;</span>;</div></td></tr>
<tr><td class="line">76</td><td><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; cube-&gt;setPosition<span class="br0">&#40;</span>x,y,z<span class="br0">&#41;</span>;</div></td></tr>
<tr><td class="line">77</td><td><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; cube-&gt;color<span class="br0">&#91;</span><span class="nu0">0</span><span class="br0">&#93;</span> = qrand<span class="br0">&#40;</span><span class="br0">&#41;</span>%<span class="nu0">255</span>;</div></td></tr>
<tr><td class="line">78</td><td><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; cube-&gt;color<span class="br0">&#91;</span><span class="nu0">1</span><span class="br0">&#93;</span> = qrand<span class="br0">&#40;</span><span class="br0">&#41;</span>%<span class="nu0">255</span>;</div></td></tr>
<tr><td class="line">79</td><td><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; cube-&gt;color<span class="br0">&#91;</span><span class="nu0">2</span><span class="br0">&#93;</span> = qrand<span class="br0">&#40;</span><span class="br0">&#41;</span>%<span class="nu0">255</span>;</div></td></tr>
<tr><td class="line">80</td><td><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; _objects.<span class="me1">push_back</span><span class="br0">&#40;</span>cube<span class="br0">&#41;</span>;</div></td></tr>
<tr><td class="line">81</td><td><div class="de1">&nbsp; &nbsp; &nbsp; <span class="br0">&#125;</span></div></td></tr>
